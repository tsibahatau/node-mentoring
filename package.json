{
  "name": "node-mentoring",
  "version": "0.0.1",
  "description": "tldr",
  "main": "index.js",
  "scripts": {
    "start:db": "docker-compose up",
    "start:nodemon": "nodemon ./src --exec babel-node",
    "setup-env": "node setup-env.js",
    "express": "nodemon ./src/express --exec babel-node",
    "build": "babel -d ./dist ./src",
    "server": "node ./dist",
    "fmt": "prettier --write src/**/*.js",
    "precommit": "lint-staged",
    "sequelize:migrate": "sequelize db:migrate",
    "sequelize:seed:all": "sequelize db:seed:all",
    "sequelize:seed:undo:all": "sequelize db:seed:undo:all",
    "sequelize:migrate:undo": "sequelize db:migrate:undo",
    "sequelize:migrate:undo:all": "sequelize db:migrate:undo:all"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsibahatau/node-mentoring.git"
  },
  "keywords": [
    "node",
    "mentoring"
  ],
  "author": "Tsimur Sibahatau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsibahatau/node-mentoring/issues"
  },
  "homepage": "https://github.com/tsibahatau/node-mentoring#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.6.1"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chokidar": "^1.7.0",
    "csvjson": "^4.3.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "jwt-redis": "^3.1.0",
    "minimist": "^1.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.0",
    "redis": "^2.8.0",
    "replacestream": "^4.0.3",
    "request": "^2.83.0",
    "sequelize": "^4.28.2",
    "sequelize-cli": "^3.2.0",
    "split2": "^2.2.0",
    "through2": "^2.0.3"
  }
}
